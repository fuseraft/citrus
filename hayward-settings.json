{
    "name": "hayward",
    "version": "1.0.0",
    "safemode": true,
    "standard_library": [
        { 
            "path": "/home/scs/GitHub/hayward/lib", 
            "autoload": true,
            "include_subdirectories": true 
        }
    ],
    "file_extensions": {
        "primary": ".hay",
        "minified": ".min.hay",
        "recognized": [
            ".min.hay", ".hay", ".kiwi",
            ".min.hayward", ".hayward", 
            ".min.🥝", ".🥝"
        ]
    },
    "debug": {
        "cli_args": [
            "/home/scs/GitHub/hayward/scripts/debug"
        ]
    },
    "environment_variables": {
        "_KIWI": ""
    },
    "crashdump_path": "/home/scs/hayward_crash.log"
}