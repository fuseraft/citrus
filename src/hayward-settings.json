{
    "name": "hayward",
    "version": "1.0.0",
    "file_extensions": {
      "primary": ".hayward",
      "minified": ".min.hayward",
      "recognized": [
        ".min.hayward",
        ".hayward",
        ".min.hay",
        ".hay",
        ".kiwi",
        ".min.🥝",
        ".🥝"
      ]
    },
    "standard_library": [
      {
        "autoload": true,
        "path": "/home/scs/GitHub/hayward/lib",
        "include_subdirectories": true
      }
    ],
    "safemode": true,
    "debug": {
      "cli_args": [
        "/home/scs/GitHub/hayward/scripts/debug"
      ]
    },
    "environment_variables": {
      "_KIWI": ""
    },
    "crashdump_path": "/home/scs/hayward_crash.log"
  }
  